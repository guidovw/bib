@article{Boskovic2009a,
	abstract = {Abstract. This reply shows that arguments against the no-DP analysis (Bokovi{\'c} 2005, 2008), according to which article-less languages do not have DP, given in Pereltsvaig 2007) do not go through. Additionally, it is shown that ordering restrictions on demonstratives, possessives, and adjectives in Serbo-Croation follow from the semantics of the elements in question. These elements can be generated in any order in the syntx, with the semantics filtering out the unacceptable orders. The goal of this reply is to address the arguments against the no-DP analysis of Bo{\v s}kovi{\'c} (2005), according to which languages without articles do not project DP (see also Bo{\v s}kovi{\'c} 2008), given in Pereltsvaig (2007).1 The latter work erroneously attributes several properties to the no-DP analysis; it also incorrectly cites Bo{\v s}kovi{\'c} (2005), attributing a statement made regarding English to a statement regarding article-less languages.},
	author = {Bo{\v s}kovi{\'c}, {\v Z}eljko},
